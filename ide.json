{
	"services": [
		{
			"active":true,
			"id":"home",
			"icon":"server",
			"name":"Home Directory",
			"type": "filesystem",
			"provider": "hermes",
			"command":"@NODE@", "params":[
				"hermes/fsLocal.js", "-P", "/files", "-p", "0", "@HOME@"
			],
			"useJsonp":false,
			"debug": false,
			"respawn": false
		},
		{
			"active":true,
			"id":"dropbox",
			"icon":"dropbox",
			"name":"Dropbox",
			"type": "filesystem",
			"provider": "hermes",
			"Xcommand":"@NODE@", "params":[
				"hermes/fsDropbox.js", "-P", "/files", "-p", "10002"
			],
			"auth": {
				"type": "dropbox",
				"appKey": "YOUR_APP_KEY",
				"appSecret": "YOUR_APP_SECRET"
			},
			"useJsonp":false,
			"debug": false
		},
		{
			"active":true,
			"id":"phonegap",
			"icon":"phonegap",
			"name":"PhoneGap Build",
			"type": "build",
			"provider": "hermes",
			"command":"@NODE@", "params":[
				"hermes/bdPhoneGap.js", "-P", "/phonegap", "-p", "0"
			],
			"useJsonp":false,
			"debug": false,
			"auth": {
				"type": "basic"
			},
			"respawn": false
		}
	]
}
